<h1>You  - Student</h1>
 <%= link_to "Add Student", {:action => 'new' } ,class: "btn btn-danger"%>
<% @students.each do |user| %>
  <div class="card mt-3" >
   <div class="card-body">
    Name : <%= user.name %>
    <%= link_to 'delete', {:action => "destroy",:id => user.id},method: :delete ,class: "btn btn-danger" %>
    <%= link_to 'edit', {:action => "edit",:id => user.id} ,class: "btn btn-info" %>
    <%= link_to "view parent", parents_path,:id => user.id ,class: "btn btn-info" %>
   </div>
  </div>
<% end %>